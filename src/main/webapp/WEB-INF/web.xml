	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>default</servlet-name>
		<url-pattern>*.unity3d</url-pattern>
	</servlet-mapping>
	<error-page>
		<error-code>401</error-code>
		<location>/unauthenticated</location>
	</error-page>
	<error-page>
		<error-code>403</error-code>
		<location>/unauthorized</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/error/error404</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/error/error500</location>
	</error-page>
	<!-- ================== 以下请不要修改 ================== -->
	<servlet>
		<servlet-name>startupLoaderBeforeSpringServlet</servlet-name>
		<servlet-class>com.supconit.common.web.startup.StartupLoaderBeforeSpringServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>startupLoaderBeforeSpringServlet</servlet-name>
		<url-pattern>/startupBeforeSpring</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>dispatcher</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>classpath:applicationContext.xml</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>dispatcher</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>startupLoaderAfterSpringServlet</servlet-name>
		<servlet-class>com.supconit.common.web.startup.StartupLoaderAfterSpringServlet</servlet-class>
		<load-on-startup>9</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>startupLoaderAfterSpringServlet</servlet-name>
		<url-pattern>/startupAfterSpring</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>gisRequestProxy</servlet-name>
		<servlet-class>com.supconit.hl.gis.servlet.GisRequestProxyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>gisRequestProxy</servlet-name>
		<url-pattern>/sendToGisServer</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	        <welcome-file>/appWindow</welcome-file>
	</welcome-file-list>	
</web-app>
